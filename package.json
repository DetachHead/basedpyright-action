{
    "name": "pyright-action",
    "version": "1.0.3",
    "private": true,
    "description": "GitHub Action for pyright",
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jakebailey/pyright-action.git"
    },
    "author": "Jake Bailey",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.6.0",
        "@actions/http-client": "^1.0.11",
        "@actions/tool-cache": "^1.7.1",
        "myzod": "^1.8.6",
        "semver": "^7.3.5",
        "string-argv": "^0.3.1"
    },
    "devDependencies": {
        "@types/node": "~12.20.28",
        "@types/semver": "^7.3.8",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "esbuild": "^0.13.4",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^7.0.2",
        "prettier": "2.4.1",
        "shx": "^0.3.3",
        "typescript": "~4.4.3"
    },
    "scripts": {
        "postinstall": "husky install",
        "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node --target=node12",
        "update:all": "yarn dlx npm-check-updates --reject 'typescript' --target minor -u && yarn",
        "update:all:transitive": "yarn dlx npm-check-updates --reject 'typescript' --target minor -u && shx rm -f yarn.lock && yarn"
    },
    "packageManager": "yarn@3.0.2"
}
