{
    "name": "pyright-action",
    "version": "1.0.8",
    "private": true,
    "description": "GitHub Action for pyright",
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jakebailey/pyright-action.git"
    },
    "author": "Jake Bailey",
    "license": "MIT",
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "@actions/core": "^1.6.0",
        "@actions/http-client": "^1.0.11",
        "@actions/tool-cache": "^1.7.2",
        "myzod": "^1.8.7",
        "semver": "^7.3.5",
        "string-argv": "^0.3.1"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.2",
        "@types/node": "^16.11.26",
        "@types/semver": "^7.3.9",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "esbuild": "^0.14.31",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^7.0.4",
        "npm-check-updates": "^12.5.7",
        "prettier": "2.6.2",
        "release-it": "^14.14.0",
        "rimraf": "^3.0.2",
        "typescript": "~4.6.3"
    },
    "scripts": {
        "postinstall": "husky install",
        "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node --target=node16",
        "update:all": "npm-check-updates --reject 'typescript' --target minor -u && yarn",
        "update:all:transitive": "npm-check-updates --reject 'typescript' --target minor -u && rimraf yarn.lock && yarn",
        "release": "release-it"
    },
    "packageManager": "yarn@3.2.0",
    "volta": {
        "node": "16.14.2"
    }
}
